<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_customer_code" model="ir.sequence">
            <field name="name">Customer Code</field>
            <field name="code">customer.code</field>
            <field name="prefix">C</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_supplier_code" model="ir.sequence">
            <field name="name">Supplier Code</field>
            <field name="code">supplier.code</field>
            <field name="prefix">S</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_sc_code" model="ir.sequence">
            <field name="name">SC Code</field>
            <field name="code">sc.code</field>
            <field name="prefix">A</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        
    </data>
</odoo>